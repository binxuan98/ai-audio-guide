/* pages/index/index.wxss */
.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
}

.header {
  padding: 16rpx;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
}

.map {
  flex: 1;
  width: 100%;
}

.bottom-bar {
  background-color: #ffffff;
  padding: 20rpx;
  border-top: 1px solid #e0e0e0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.guide-title {
  font-size: 32rpx;
}

  